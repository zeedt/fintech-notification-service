<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>jfrog-central</id>
            <username>${JFROG_USERNAME}</username>
            <password>${JFROG_PASSWORD}</password>
        </server>
        <server>
            <id>jfrog-snapshots</id>
            <username>${JFROG_USERNAME}</username>
            <password>${JFROG_PASSWORD}</password>
        </server>
    </servers>

    <profiles>
        <profile>
            <repositories>
                <repository>
                    <id>jfrog-central</id>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <name>a0bxebnfnzxe5-artifactory-primary-0-releases</name>
                    <url>https://zeedt.jfrog.io/artifactory/fintech-nigeria-app</url>
                </repository>
                <repository>
                    <id>jfrog-snapshots</id>
                    <snapshots />
                    <name>a0bxebnfnzxe5-artifactory-primary-0-snapshots</name>
                    <url>https://zeedt.jfrog.io/artifactory/fintech-nigeria-app</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <offline>false</offline>
</settings>